@if (TempData.ContainsKey(WebConstants.TempDataSuccessMessageKey))
{
    <script>
        toastr.success('@TempData[WebConstants.TempDataSuccessMessageKey]', 'Success', { positionClass: 'toast-top-center' });
    </script>
}

@if (TempData.ContainsKey(WebConstants.TempDataErrorMessageKey))
{
    <script>
        toastr.error('@TempData[WebConstants.TempDataErrorMessageKey]', 'Error', { positionClass: 'toast-top-center' });
    </script>
}