@if (TempData.ContainsKey(WebConstants.TempDataSuccessMessageKey))
{
    <script>
        toastr.success('@TempData[WebConstants.TempDataSuccessMessageKey]', 'Success');
    </script>
}

@if (TempData.ContainsKey(WebConstants.TempDataErrorMessageKey))
{
    <script>
        toastr.error('@TempData[WebConstants.TempDataErrorMessageKey]', 'Error');
    </script>
}

@if (TempData.ContainsKey(WebConstants.TempDataInfoMessageKey))
{
    <script>
        toastr.error('@TempData[WebConstants.TempDataInfoMessageKey]', 'Info');
    </script>
}